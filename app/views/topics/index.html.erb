<p id="notice"><%= notice %></p>

<h1>Topics</h1>

<table>
  <thead>
    <tr>
      <td>Id</td>
      <td>Topics</td>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @topics.each do |t| %>
      <tr>
        <td><%= t.id %></td>
        <td><%= t.topic %></td>
      </tr>
    <% end %>
  </tbody>
</table>

